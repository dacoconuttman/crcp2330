/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;
    PARTS:
    NOT(in = a, out = nota);
    NOT(in = b, out = notb);
    AND(a = a, b = notb, out = and1);
    AND(a = nota, b = b, out = and2);
    OR(a = and1, b = and2, out = out);
}
