/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    NOT(in = s, out = n1);
    AND(a = a, b = n1, out = a1);
    AND(a = s, b = b, out = a2);
    OR(a = a1, b = a2, out = out);
}
